#inisiasi list
my_list = ["prodi TI",1,2,3,4,1,2]
#cetak list
print("my_list: ",my_list)
print("panggil index ke 5: ", my_list[5])
#tambah anggota di dalam list
my_list.append("keren")
#cetak list setelah input anggota baru
print("my list anggota baru",my_list)
#menghapus nilai 2 dalam list
my_list.remove(2)
#cetak list setelah menghapus nilai 2 pertama
print("my_list setelah menghapus nilai 2: ", my_list)
#menghapus index 1
del my_list[1]
#cetak my list setelah menghapus index 1
print("my_list setelah menghapus index : ", my_list)

